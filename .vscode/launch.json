{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C++(CONSOLE)",
      "type": "cppvsdbg",
      "preLaunchTask": "C/C++: g++.exe build active file",
      "request": "launch",
      "program": "${fileBasenameNoExtension}.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "console": "externalTerminal"
    },
    {
      "name": "Python(CONSOLE)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "externalTerminal",
      "justMyCode": true
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/Users/danil/Programs",
      "program": "c:/Users/danil/Programs/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}